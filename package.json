    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "cssnano": {
        "preset": [
            "default",
            {
                "calc": false
            }
        ]
    },
    "overrides": {
        "typescript": "^5.2.2",
        "fork-ts-checker-webpack-plugin": "^6.5.3",
